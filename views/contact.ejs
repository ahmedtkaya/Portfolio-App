<%- include('partials/_header.ejs') %>
    <div class="tm-page-container mx-auto">
        <header class="tm-header text-center">
            <h1 class="tm-title text-uppercase">Contact</h1>
            <p class="tm-primary-color"><i>You Can Contact With Me</i></p>
        </header>

        <section class="tm-section">
            <%- include('partials/_navigation.ejs') %>
            <div class="tm-content-container">
                <div class="mb-0 tm-img-overlay-wrap">
                    <div class="tm-img-overlay"></div>
                    <div class="tm-img-overlay-text text-white p-5">
                        <h4 class="mb-4">Contact With Me</h4>
                        
                    </div>
                </div>
                <div class="tm-content">
                    <form action="/contact" method="POST" class="tm-contact-form">
                        <div class="form-group">
                            <input type="text" id="name" name="name"
                                class="form-control rounded-0 border-top-0 border-right-0 border-left-0"
                                placeholder="Your Name" />
                        </div>
                        <div class="form-group">
                            <input type="text" id="email" name="email"
                                class="form-control rounded-0 border-top-0 border-right-0 border-left-0"
                                placeholder="Your Email"  />
                        </div>
                        <div class="form-group">
                            <textarea type="text" id="message" name="message"
                                class="form-control rounded-0 border-top-0 border-right-0 border-left-0"
                                placeholder="Your Message"  ></textarea>
                        </div>

                        

                        <div class="form-group text-right">
                            <button type="submit" class="btn btn-primary rounded-0" value="LOGIN">
                                LOGIN
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </section>

        <%- include('partials/_footer.ejs') %>
    </div>
</body>

</html>